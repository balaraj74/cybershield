# CyberShield AI - Environment Variables
# Copy this file to .env.local and configure for your environment

# Authentication
AUTH_SECRET="cybershield-dev-secret-key-at-least-32-characters-long"
AUTH_URL="http://localhost:3000"

# FastAPI Backend
FASTAPI_URL="http://localhost:8001"
FASTAPI_API_KEY="cybershield-api-key-2024"

# Application Mode
NEXT_PUBLIC_APP_NAME="CyberShield AI"
NEXT_PUBLIC_DEMO_MODE=false

# Supabase (for client-side features)
NEXT_PUBLIC_SUPABASE_URL="https://[PROJECT-REF].supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# Skip environment validation (for CI/CD)
# SKIP_ENV_VALIDATION=true

